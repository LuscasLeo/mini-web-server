{
	"name": "raw-react",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start:client": "webpack serve --hot --config ./config/webpack.config.client.js",
		"start": "npm-run-all --parallel start:server start:client",
		"start:server": "cross-env NODE_ENV=development UNCOMPILED=true ts-node-dev src/server/index.ts",
		"build:p": "npm-run-all build:p:*",
		"build:p:client": "webpack --mode production --config ./config/webpack.config.client.js --progress",
		"build:p:server": "tsc --project src\\server\\tsconfig.json",
		"build:d": "npm-run-all build:d:*",
		"build:d:client": "webpack --mode development --config ./config/webpack.config.client.js --progress",
		"build:d:server": "webpack --mode development --config ./config/webpack.config.server.js --progress",
		"package": "node ./src/compiler.js",
		"package:b": "npm-run-all build:p package",
		"postinstall": "node postinstall.js",
		"clean": "rd dist /s/q",
		"clean:server": "rd dist\\server /s/q",
		"clean:client": "rd dist\\client /s/q"
	},
	"devDependencies": {
